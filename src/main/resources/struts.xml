<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <!--<constant name="struts.devMode" value="true" />-->
    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <package name="default" namespace="/" extends="struts-default">
        <action name="PL01" class="com.lw.action.PL01">
            <result name="success">/pagelevel01.jsp</result>
        </action>
        <action name="PL03" class="com.lw.action.PL03">
           <result name="success">/pagelevel02.jsp</result>
        </action>
        <action name="PL03S" class="com.lw.action.PL03" method="selected">
            <result name="selected">/pagelevel02.jsp</result>
        </action>
        <action name="PL01S" class="com.lw.action.PL01" method="selected">
            <result name="success">/pagelevel01.jsp</result>
        </action>
        <action name="PL03G" class="com.lw.action.PL03" method="go">
            <result name="success">//pagelevel04.jsp</result>
        </action>
        <action name="PL03U" class="com.lw.action.PL03" method="update">
            <result name="success" type="redirect">/PL03S.action</result>
        </action>
        <action name="PL02" class="com.lw.action.PL02" method="execute">
            <result name="success">/pagelevel05.jsp</result>
        </action>
    </package>
</struts>